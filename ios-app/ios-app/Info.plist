<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDisplayName</key>
	<string>Medkit</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>firstaidcoach</string>
			</array>
		</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>fb-viewapp</string>
	</array>
	<key>MWDAT</key>
	<dict>
		<key>AppLinkURLScheme</key>
		<string>firstaidcoach://</string>
		<key>ClientToken</key>
		<string></string>
		<key>MetaAppID</key>
		<string></string>
		<key>TeamID</key>
		<string>$(DEVELOPMENT_TEAM)</string>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>Needed to capture video from your phone camera when glasses aren't connected.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Needed to connect to Meta Wearables</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Needed to connect to Meta Wearables</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Used to center the map on your location and find nearby AEDs and hospitals.</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>Needed to connect to the coaching backend server.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Needed to capture audio from your glasses microphone.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>This app needs access to save photos captured from your glasses.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>Needed to detect the "Medkit" wake word.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>bluetooth-peripheral</string>
		<string>external-accessory</string>
	</array>
	<key>UILaunchScreen</key>
	<dict/>
	<key>UIRequiresFullScreen</key>
	<true/>
	<key>UISupportedExternalAccessoryProtocols</key>
	<array>
		<string>com.meta.ar.wearable</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>
</dict>
</plist>
